{
    "name": "acsolution",
    "version": "1.0.0",
    "description": "ACSOLUTION - Sistema de Gestión de Materiales y Órdenes de Trabajo",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "build": "node build.js",
        "build:frontend": "cd frontend && npm install && npm run build",
        "build:backend": "cd backend && npm install --legacy-peer-deps && npm run build",
        "dev": "cd backend && npm run start:dev",
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && npm run start:dev",
        "start:backend": "cd backend && npm run start:prod"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "express": "^4.18.2",
        "http-proxy-middleware": "^2.0.6"
    },
    "private": true
}